
var mongoose = require('mongoose'),
	crypto = require('crypto');

var usuariosSchema = new mongoose.Schema({

	id_usuarios: ObjectID,
	nome: String,
	sobrenome: String,
	email: String,
	telefone: String,
	senha:String

},{collection: 'usuarios'})


usuariosSchema

module.exports.mUsuarios = mongoose.model('usuarios', usuariosSchema);